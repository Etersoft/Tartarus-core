def build(bld):
    sbins = (
            'Tartarus-srv1',
            'Tartarus-join',
            'Tartarus-leave',
            't-deploy-srv',
            't-modules',
            't-join')
    bins = set(bld.glob('t-*'))
    bins -= frozenset(sbins)
    bld.install_files('${BINDIR}', bins, chmod=0755)
    bld.install_files('${SBINDIR}', sbins, chmod=0755)

