#
# The client reads this property to create the reference to the
# object in the server.
#
Tartarus.deployPrx.DNSService      = Service/DNS:          tcp -p 12345
Tartarus.deployPrx.DNS             = DNS/Server:           tcp -p 12345
Tartarus.deployPrx.GroupManager    = SysDB-Manager/Groups: tcp -p 12345
Tartarus.deployPrx.Kadmin          = Kerberos/Kadmin:      tcp -p 12345
Tartarus.deployPrx.KerberosService = Service/Kerberos:     tcp -p 12345
Tartarus.deployPrx.SysDBService    = Service/SysDB:        tcp -p 12345
Tartarus.deployPrx.UserManager     = SysDB-Manager/Users:  tcp -p 12345

#
# Warn about connection exceptions
#
#Ice.Warn.Connections=1

#
# Network Tracing
#
# 0 = no network tracing
# 1 = trace connection establishment and closure
# 2 = like 1, but more detailed
# 3 = like 2, but also trace data transfer
#
#Ice.Trace.Network=1

#
# Protocol Tracing
#
# 0 = no protocol tracing
# 1 = trace protocol messages
#
#Ice.Trace.Protocol=1

#
# Security Tracing
#
# 0 = no security tracing
# 1 = trace messages
#
#IceSSL.Trace.Security=1

#
# SSL Configuration
#
Ice.Plugin.IceSSL=IceSSL:createIceSSL
IceSSL.Ciphers=KRB5
IceSSL.ServiceHost=evilistiq.saratov.etersoft.ru
